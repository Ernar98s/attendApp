AA,aAAA,GAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,GAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAFA,EAEA,IAFA,CAAA;AAGA,OAtBA;AAuBA,KA3BA;AA2BA,IAAA,UAAA,EAAA,oBAAA,MAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA;AACA,WAAA,MAAA,GAAA,MAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,GAAA,CAAA,MAAA,GAAA,EAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA,KAjCA;AAkCA,IAAA,WAAA,EAAA,qBAAA,OAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA;AACA,MAAA,GAAA,CAAA,OAAA,GAAA,OAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,GAAA,CAAA,OAAA,GAAA,EAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA;AAxCA,GAZA;AAsDA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA;AAtDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"c-app flex-row align-items-center\">\r\n    <CContainer>\r\n      <CRow class=\"justify-content-center\">\r\n        <CCol md=\"8\">\r\n          <CCardGroup>\r\n            <CCard class=\"p-4\">\r\n              <CCardBody>\r\n                <form v-on:submit=\"loginAuth\">\r\n                  <h1>Вход</h1>\r\n                  <p class=\"text-muted\">Введите свои данные, чтобы зайти в Ваш аккаунт</p>\r\n                  <CInput\r\n                      placeholder=\"Логин\"\r\n                      autocomplete=\"username email\"\r\n                      required\r\n                      v-model=\"email\"\r\n                  >\r\n                    <template #prepend-content>\r\n                      <CIcon name=\"cil-user\"/>\r\n                    </template>\r\n                  </CInput>\r\n                  <CInput\r\n                      placeholder=\"Пароль\"\r\n                      type=\"password\"\r\n                      autocomplete=\"curent-password\"\r\n                      required\r\n                      v-model=\"password\"\r\n                  >\r\n                    <template #prepend-content>\r\n                      <CIcon name=\"cil-lock-locked\"/>\r\n                    </template>\r\n                  </CInput>\r\n                  <CRow>\r\n                    <CCol col=\"6\" class=\"text-left\">\r\n                      <CButton type=\"submit\" color=\"primary\" class=\"px-4\">Войти</CButton>\r\n                    </CCol>\r\n                    <CCol col=\"6\" class=\"text-right\">\r\n\r\n\r\n                    </CCol>\r\n                  </CRow>\r\n                </form>\r\n              </CCardBody>\r\n            </CCard>\r\n            <CCard\r\n                color=\"primary\"\r\n                text-color=\"white\"\r\n                class=\"text-center py-5 d-md-down-none\"\r\n                body-wrapper\r\n            >\r\n              <CCardBody>\r\n                <h2>Регистрация</h2>\r\n                <p>Обратитесь к главному менеджеру для регистрации.</p>\r\n\r\n              </CCardBody>\r\n            </CCard>\r\n          </CCardGroup>\r\n        </CCol>\r\n      </CRow>\r\n    </CContainer>\r\n    <div class=\"alert_block\">\r\n      <CAlert color=\"warning\" closeButton v-if=\"warning\">\r\n        {{ alert_message }}\r\n      </CAlert>\r\n      <CAlert color=\"primary\" closeButton v-if=\"success\">\r\n        {{ alert_message }}\r\n      </CAlert>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport alert from '../../components/alert/alert-block';\r\nimport appRouter from '@/router'\r\n\r\nconst router = appRouter\r\nconst qs = require('qs')\r\nconst axios = require('axios')\r\n\r\nexport default {\r\n  name: 'Login',\r\n  data() {\r\n    return {\r\n      warning: false,\r\n      success: false,\r\n      alert_message: '',\r\n      email: '',\r\n      password: '',\r\n      type: 1,\r\n    };\r\n  },\r\n  methods: {\r\n    loginAuth() {\r\n      event.preventDefault();\r\n      let app = this;\r\n      axios(\r\n          {\r\n            method: 'post',\r\n            url: '/token',\r\n            data: qs.stringify({\r\n              username: this.email,\r\n              password: this.password\r\n            }),\r\n            headers: {\r\n              'content-type': 'application/x-www-form-urlencoded;charset=utf-8'\r\n            }\r\n          })\r\n          .then(({data}) => {\r\n\r\n            window.auth.login(data.access_token);\r\n            router.push({path: '/dashboard'})\r\n          }).catch(function (error) {\r\n        app.success = true;\r\n        app.alert_message = error.response.data.detail;\r\n        setTimeout(function () {\r\n          app.success = false;\r\n        }, 3000)\r\n      })\r\n    }, showErrors: function (errors) {\r\n      let app = this;\r\n      this.errors = errors;\r\n      setTimeout(function () {\r\n        app.errors = {};\r\n      }, 3000)\r\n    },\r\n    showSuccess: function (message) {\r\n      let app = this;\r\n      app.success = message;\r\n      setTimeout(function () {\r\n        app.success = '';\r\n      }, 3000)\r\n    },\r\n  },\r\n  components: {\r\n    alert,\r\n  }\r\n}\r\n</script>\r\n<style>\r\n\r\n.alert_block {\r\n  position: fixed;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/pages"}]}