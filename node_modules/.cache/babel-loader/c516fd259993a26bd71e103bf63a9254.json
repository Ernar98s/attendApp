AACA,0BAAA;AADA;AAPA,OADA,CAAA,CAYA,IAZA,CAYA,gBAAA;AAAA,YAAA,IAAA,QAAA,IAAA;AAEA,QAAA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OAhBA,EAgBA,KAhBA,CAgBA,UAAA,KAAA,EAAA;AACA,QAAA,GAAA,CAAA,OAAA,GAAA,IAAA;AACA,QAAA,GAAA,CAAA,aAAA,GAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,GAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAFA,EAEA,IAFA,CAAA;AAGA,OAtBA;AAuBA,KA3BA;AA2BA,IAAA,UAAA,EAAA,oBAAA,MAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA;AACA,WAAA,MAAA,GAAA,MAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,GAAA,CAAA,MAAA,GAAA,EAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA,KAjCA;AAkCA,IAAA,WAAA,EAAA,qBAAA,OAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA;AACA,MAAA,GAAA,CAAA,OAAA,GAAA,OAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,GAAA,CAAA,OAAA,GAAA,EAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA;AAxCA,GAZA;AAsDA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA;AAtDA,CAAA","sourcesContent":["<template>\n  <div class=\"c-app flex-row align-items-center\">\n    <CContainer>\n      <CRow class=\"justify-content-center\">\n        <CCol md=\"8\">\n          <CCardGroup>\n            <CCard class=\"p-4\">\n              <CCardBody>\n                <form v-on:submit=\"loginAuth\">\n                  <h1>Вход</h1>\n                  <p class=\"text-muted\">Введите свои данные, чтобы зайти в Ваш аккаунт</p>\n                  <CInput\n                      placeholder=\"Логин\"\n                      autocomplete=\"username email\"\n                      required\n                      v-model=\"email\"\n                  >\n                    <template #prepend-content>\n                      <CIcon name=\"cil-user\"/>\n                    </template>\n                  </CInput>\n                  <CInput\n                      placeholder=\"Пароль\"\n                      type=\"password\"\n                      autocomplete=\"curent-password\"\n                      required\n                      v-model=\"password\"\n                  >\n                    <template #prepend-content>\n                      <CIcon name=\"cil-lock-locked\"/>\n                    </template>\n                  </CInput>\n                  <CRow>\n                    <CCol col=\"6\" class=\"text-left\">\n                      <CButton type=\"submit\" color=\"primary\" class=\"px-4\">Войти</CButton>\n                    </CCol>\n                    <CCol col=\"6\" class=\"text-right\">\n\n\n                    </CCol>\n                  </CRow>\n                </form>\n              </CCardBody>\n            </CCard>\n            <CCard\n                color=\"primary\"\n                text-color=\"white\"\n                class=\"text-center py-5 d-md-down-none\"\n                body-wrapper\n            >\n              <CCardBody>\n                <h2>Регистрация</h2>\n                <p>Обратитесь к главному менеджеру для регистрации.</p>\n\n              </CCardBody>\n            </CCard>\n          </CCardGroup>\n        </CCol>\n      </CRow>\n    </CContainer>\n    <div class=\"alert_block\">\n      <CAlert color=\"warning\" closeButton v-if=\"warning\">\n        {{ alert_message }}\n      </CAlert>\n      <CAlert color=\"primary\" closeButton v-if=\"success\">\n        {{ alert_message }}\n      </CAlert>\n    </div>\n  </div>\n</template>\n\n<script>\nimport alert from '../../components/alert/alert-block';\nimport appRouter from '@/router'\n\nconst router = appRouter\nconst qs = require('qs')\nconst axios = require('axios')\n\nexport default {\n  name: 'Login',\n  data() {\n    return {\n      warning: false,\n      success: false,\n      alert_message: '',\n      email: '',\n      password: '',\n      type: 1,\n    };\n  },\n  methods: {\n    loginAuth() {\n      event.preventDefault();\n      let app = this;\n      axios(\n          {\n            method: 'post',\n            url: '/token',\n            data: qs.stringify({\n              username: this.email,\n              password: this.password\n            }),\n            headers: {\n              'content-type': 'application/x-www-form-urlencoded;charset=utf-8'\n            }\n          })\n          .then(({data}) => {\n\n            window.auth.login(data.access_token);\n            router.push({path: '/dashboard'})\n          }).catch(function (error) {\n        app.success = true;\n        app.alert_message = error.response.data.detail;\n        setTimeout(function () {\n          app.success = false;\n        }, 3000)\n      })\n    }, showErrors: function (errors) {\n      let app = this;\n      this.errors = errors;\n      setTimeout(function () {\n        app.errors = {};\n      }, 3000)\n    },\n    showSuccess: function (message) {\n      let app = this;\n      app.success = message;\n      setTimeout(function () {\n        app.success = '';\n      }, 3000)\n    },\n  },\n  components: {\n    alert,\n  }\n}\n</script>\n<style>\n\n.alert_block {\n  position: fixed;\n  right: 0;\n  bottom: 0;\n}\n</style>\n"],"sourceRoot":"src/views/pages"}]}